{
  "name": "node-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/index.js",
    "start:dev": "nodemon --watch 'src/**/*.js' index.js",
    "lint": "eslint \"{src,test}/**/*.js\" --fix",
    "pretty": "prettier --write '**/*.{js,json}' '!node_modules/**'",
    "postpretty": "lint --fix"
  },
  "dependencies": {
    "api": "^6.1.1",
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-status-codes": "^2.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2"
  },
  "_moduleAliases": {
    "@src": "./src"
  }
}
